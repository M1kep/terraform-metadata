{
  "name": "dnsimple",
  "type": "provider",
  "version": "v0.1.0-13-gae3e59e",
  "provider": {
    "account": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The account for API operations."
    },
    "email": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The DNSimple account email address.",
      "Default": {
        "Type": "string"
      }
    },
    "token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The API v2 token for API operations."
    }
  },
  "resources": {
    "dnsimple_record": {
      "domain": {
        "Type": "TypeString",
        "Required": true
      },
      "domain_id": {
        "Type": "TypeString",
        "Computed": true
      },
      "hostname": {
        "Type": "TypeString",
        "Computed": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "priority": {
        "Type": "TypeString",
        "Optional": true,
        "Computed": true
      },
      "ttl": {
        "Type": "TypeString",
        "Optional": true
      },
      "type": {
        "Type": "TypeString",
        "Required": true
      },
      "value": {
        "Type": "TypeString",
        "Required": true
      }
    }
  },
  "data-sources": {}
}