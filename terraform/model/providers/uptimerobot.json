{
  ".schema_version": "2",
  ".sdk_type": "terraform-plugin-sdk-v1",
  "name": "uptimerobot",
  "type": "provider",
  "version": "v0.5.1",
  "provider": {
    "api_key": {
      "Type": "String",
      "Required": true,
      "DefaultFunc": "ENV(UPTIMEROBOT_API_KEY)"
    }
  },
  "resources": {
    "uptimerobot_alert_contact": {
      "friendly_name": {
        "Type": "String",
        "Required": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "value": {
        "Type": "String",
        "Required": true
      }
    },
    "uptimerobot_monitor": {
      "alert_contact": {
        "Type": "List",
        "Optional": true,
        "IsBlock": true,
        "ConfigImplicitMode": "Block",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "id": {
              "Type": "String",
              "Required": true
            },
            "recurrence": {
              "Type": "Int",
              "Optional": true
            },
            "threshold": {
              "Type": "Int",
              "Optional": true
            }
          }
        }
      },
      "custom_http_headers": {
        "Type": "Map",
        "Optional": true
      },
      "friendly_name": {
        "Type": "String",
        "Required": true
      },
      "http_auth_type": {
        "Type": "String",
        "Optional": true
      },
      "http_password": {
        "Type": "String",
        "Optional": true
      },
      "http_username": {
        "Type": "String",
        "Optional": true
      },
      "interval": {
        "Type": "Int",
        "Optional": true,
        "Default": {
          "Type": "int",
          "Value": "300"
        }
      },
      "keyword_type": {
        "Type": "String",
        "Optional": true
      },
      "keyword_value": {
        "Type": "String",
        "Optional": true
      },
      "port": {
        "Type": "Int",
        "Optional": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "sub_type": {
        "Type": "String",
        "Optional": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "url": {
        "Type": "String",
        "Required": true
      }
    },
    "uptimerobot_status_page": {
      "custom_domain": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string"
        }
      },
      "custom_url": {
        "Type": "String",
        "Computed": true
      },
      "dns_address": {
        "Type": "String",
        "Computed": true
      },
      "friendly_name": {
        "Type": "String",
        "Required": true
      },
      "monitors": {
        "Type": "List",
        "Optional": true,
        "PromoteSingle": true,
        "ConfigImplicitMode": "Attr",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "Int"
        }
      },
      "password": {
        "Type": "String",
        "Optional": true
      },
      "sort": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "a-z"
        }
      },
      "standard_url": {
        "Type": "String",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Optional": true,
        "Default": {
          "Type": "string",
          "Value": "active"
        }
      }
    }
  },
  "data-sources": {
    "uptimerobot_account": {
      "down_monitors": {
        "Type": "Int",
        "Computed": true
      },
      "email": {
        "Type": "String",
        "Computed": true
      },
      "monitor_interval": {
        "Type": "Int",
        "Computed": true
      },
      "monitor_limit": {
        "Type": "Int",
        "Computed": true
      },
      "paused_monitors": {
        "Type": "Int",
        "Computed": true
      },
      "up_monitors": {
        "Type": "Int",
        "Computed": true
      }
    },
    "uptimerobot_alert_contact": {
      "friendly_name": {
        "Type": "String",
        "Optional": true
      },
      "id": {
        "Type": "String",
        "Computed": true
      },
      "status": {
        "Type": "String",
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Computed": true
      },
      "value": {
        "Type": "String",
        "Optional": true
      }
    }
  }
}