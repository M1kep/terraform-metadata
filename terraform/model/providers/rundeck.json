{
  "name": "rundeck",
  "type": "provider",
  "version": "v0.1.0-15-gcfea3ec",
  "provider": {
    "allow_unverified_ssl": {
      "Type": "TypeBool",
      "Optional": true,
      "Description": "If set, the Rundeck client will permit unverifiable SSL certificates."
    },
    "auth_token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "Auth token to use with the Rundeck API."
    },
    "url": {
      "Type": "TypeString",
      "Required": true,
      "Description": "URL of the root of the target Rundeck server."
    }
  },
  "resources": {
    "rundeck_job": {
      "allow_concurrent_executions": {
        "Type": "TypeBool",
        "Optional": true
      },
      "command": {
        "Type": "TypeList",
        "Required": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "description": {
              "Type": "TypeString",
              "Optional": true
            },
            "inline_script": {
              "Type": "TypeString",
              "Optional": true
            },
            "job": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "args": {
                    "Type": "TypeString",
                    "Optional": true
                  },
                  "group_name": {
                    "Type": "TypeString",
                    "Optional": true
                  },
                  "name": {
                    "Type": "TypeString",
                    "Required": true
                  },
                  "run_for_each_node": {
                    "Type": "TypeBool",
                    "Optional": true
                  }
                }
              }
            },
            "node_step_plugin": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "config": {
                    "Type": "TypeMap",
                    "Optional": true
                  },
                  "type": {
                    "Type": "TypeString",
                    "Required": true
                  }
                }
              }
            },
            "script_file": {
              "Type": "TypeString",
              "Optional": true
            },
            "script_file_args": {
              "Type": "TypeString",
              "Optional": true
            },
            "shell_command": {
              "Type": "TypeString",
              "Optional": true
            },
            "step_plugin": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "config": {
                    "Type": "TypeMap",
                    "Optional": true
                  },
                  "type": {
                    "Type": "TypeString",
                    "Required": true
                  }
                }
              }
            }
          }
        }
      },
      "command_ordering_strategy": {
        "Type": "TypeString",
        "Optional": true
      },
      "continue_on_error": {
        "Type": "TypeBool",
        "Optional": true
      },
      "description": {
        "Type": "TypeString",
        "Required": true
      },
      "group_name": {
        "Type": "TypeString",
        "Optional": true
      },
      "id": {
        "Type": "TypeString",
        "Computed": true
      },
      "log_level": {
        "Type": "TypeString",
        "Optional": true
      },
      "max_thread_count": {
        "Type": "TypeInt",
        "Optional": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "node_filter_exclude_precedence": {
        "Type": "TypeBool",
        "Optional": true
      },
      "node_filter_query": {
        "Type": "TypeString",
        "Optional": true
      },
      "option": {
        "Type": "TypeList",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "allow_multiple_values": {
              "Type": "TypeBool",
              "Optional": true
            },
            "default_value": {
              "Type": "TypeString",
              "Optional": true
            },
            "description": {
              "Type": "TypeString",
              "Optional": true
            },
            "exposed_to_scripts": {
              "Type": "TypeBool",
              "Optional": true
            },
            "multi_value_delimiter": {
              "Type": "TypeString",
              "Optional": true
            },
            "name": {
              "Type": "TypeString",
              "Required": true
            },
            "obscure_input": {
              "Type": "TypeBool",
              "Optional": true
            },
            "require_predefined_choice": {
              "Type": "TypeBool",
              "Optional": true
            },
            "required": {
              "Type": "TypeBool",
              "Optional": true
            },
            "validation_regex": {
              "Type": "TypeString",
              "Optional": true
            },
            "value_choices": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "TypeString"
              }
            },
            "value_choices_url": {
              "Type": "TypeString",
              "Optional": true
            }
          }
        }
      },
      "preserve_options_order": {
        "Type": "TypeBool",
        "Optional": true,
        "Computed": true
      },
      "project_name": {
        "Type": "TypeString",
        "Required": true
      },
      "rank_attribute": {
        "Type": "TypeString",
        "Optional": true
      },
      "rank_order": {
        "Type": "TypeString",
        "Optional": true
      },
      "schedule": {
        "Type": "TypeString",
        "Optional": true
      }
    },
    "rundeck_private_key": {
      "key_material": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The private key material to store, in PEM format"
      },
      "path": {
        "Type": "TypeString",
        "Required": true,
        "Description": "Path to the key within the key store"
      }
    },
    "rundeck_project": {
      "default_node_executor_plugin": {
        "Type": "TypeString",
        "Optional": true
      },
      "default_node_file_copier_plugin": {
        "Type": "TypeString",
        "Optional": true
      },
      "description": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "Description of the project to be shown in the Rundeck UI"
      },
      "extra_config": {
        "Type": "TypeMap",
        "Optional": true,
        "Description": "Additional raw configuration parameters to include in the project configuration, with dots replaced with slashes in the key names due to limitations in Terraform's config language."
      },
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "Unique name for the project"
      },
      "resource_model_source": {
        "Type": "TypeList",
        "Required": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "config": {
              "Type": "TypeMap",
              "Required": true,
              "Description": "Configuration parameters for the selected plugin"
            },
            "type": {
              "Type": "TypeString",
              "Required": true,
              "Description": "Name of the resource model plugin to use"
            }
          }
        }
      },
      "ssh_authentication_type": {
        "Type": "TypeString",
        "Optional": true
      },
      "ssh_key_file_path": {
        "Type": "TypeString",
        "Optional": true
      },
      "ssh_key_storage_path": {
        "Type": "TypeString",
        "Optional": true
      },
      "ui_url": {
        "Type": "TypeString",
        "Computed": true
      }
    },
    "rundeck_public_key": {
      "delete": {
        "Type": "TypeBool",
        "Description": "True if the key should be deleted when the resource is deleted. Defaults to true if key_material is provided in the configuration.",
        "Computed": true
      },
      "key_material": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The public key data to store, in the usual OpenSSH public key file format",
        "Computed": true
      },
      "path": {
        "Type": "TypeString",
        "Required": true,
        "Description": "Path to the key within the key store"
      },
      "url": {
        "Type": "TypeString",
        "Description": "URL at which the key content can be retrieved",
        "Computed": true
      }
    }
  },
  "data-sources": {}
}