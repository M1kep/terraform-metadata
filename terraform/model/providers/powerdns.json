{
  ".schema_version": "1",
  "name": "powerdns",
  "type": "provider",
  "version": "v0.1.0",
  "provider": {
    "api_key": {
      "Type": "String",
      "Required": true,
      "Description": "REST API authentication key",
      "DefaultFunc": "ENV"
    },
    "server_url": {
      "Type": "String",
      "Required": true,
      "Description": "Location of PowerDNS server",
      "DefaultFunc": "ENV"
    }
  },
  "resources": {
    "powerdns_record": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "records": {
        "Type": "Set",
        "Required": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "String"
        }
      },
      "ttl": {
        "Type": "Int",
        "Required": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "zone": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}