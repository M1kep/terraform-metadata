{
  ".schema_version": "1",
  "name": "dyn",
  "type": "provider",
  "version": "v1.1.0",
  "provider": {
    "customer_name": {
      "Type": "String",
      "Required": true,
      "Description": "A Dyn customer name.",
      "DefaultFunc": "ENV(DYN_CUSTOMER_NAME)"
    },
    "password": {
      "Type": "String",
      "Required": true,
      "Description": "The Dyn password.",
      "DefaultFunc": "ENV(DYN_PASSWORD)"
    },
    "username": {
      "Type": "String",
      "Required": true,
      "Description": "A Dyn username.",
      "DefaultFunc": "ENV(DYN_USERNAME)"
    }
  },
  "resources": {
    "dyn_record": {
      "fqdn": {
        "Type": "String",
        "Computed": true
      },
      "name": {
        "Type": "String",
        "Optional": true
      },
      "ttl": {
        "Type": "String",
        "Optional": true,
        "Computed": true
      },
      "type": {
        "Type": "String",
        "Required": true
      },
      "value": {
        "Type": "String",
        "Required": true
      },
      "zone": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}