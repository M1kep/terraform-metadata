{
  ".schema_version": "1",
  "name": "dns",
  "type": "provider",
  "version": "v2.1.0",
  "provider": {
    "update": {
      "Type": "List",
      "Optional": true,
      "MaxItems": 1,
      "Elem": {
        "Type": "SchemaInfo",
        "Info": {
          "key_algorithm": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYALGORITHM)"
          },
          "key_name": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYNAME)"
          },
          "key_secret": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_KEYSECRET)"
          },
          "port": {
            "Type": "Int",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "retries": {
            "Type": "Int",
            "Optional": true,
            "DefaultFunc": "UNKNOWN"
          },
          "server": {
            "Type": "String",
            "Required": true,
            "DefaultFunc": "ENV(DNS_UPDATE_SERVER)"
          },
          "timeout": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_TIMEOUT)"
          },
          "transport": {
            "Type": "String",
            "Optional": true,
            "DefaultFunc": "ENV(DNS_UPDATE_TRANSPORT)"
          }
        }
      }
    }
  },
