{
  "name": "arukas",
  "type": "provider",
  "version": "v0.1.0-12-g4293f3a",
  "provider": {
    "api_url": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "default Arukas API url",
      "Default": {
        "Type": "string",
        "Value": "https://app.arukas.io/api/"
      }
    },
    "secret": {
      "Type": "TypeString",
      "Required": true,
      "Description": "your Arukas APIKey(secret)"
    },
    "timeout": {
      "Type": "TypeInt",
      "Optional": true,
      "Default": {
        "Type": "string",
        "Value": "900"
      }
    },
    "token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "your Arukas APIKey(token)"
    },
    "trace": {
      "Type": "TypeString",
      "Optional": true,
      "Default": {
        "Type": "string"
      }
    }
  },
  "resources": {
    "arukas_container": {
      "app_id": {
        "Type": "TypeString",
        "Computed": true
      },
      "cmd": {
        "Type": "TypeString",
        "Optional": true
      },
      "endpoint": {
        "Type": "TypeString",
        "Optional": true,
        "Computed": true
      },
      "endpoint_full_hostname": {
        "Type": "TypeString",
        "Computed": true
      },
      "endpoint_full_url": {
        "Type": "TypeString",
        "Computed": true
      },
      "environments": {
        "Type": "TypeList",
        "Optional": true,
        "MaxItems": 20,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "key": {
              "Type": "TypeString",
              "Required": true
            },
            "value": {
              "Type": "TypeString",
              "Required": true
            }
          }
        }
      },
      "image": {
        "Type": "TypeString",
        "Required": true
      },
      "instances": {
        "Type": "TypeInt",
        "Optional": true
      },
      "memory": {
        "Type": "TypeInt",
        "Optional": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "port_mappings": {
        "Type": "TypeList",
        "Computed": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "container_port": {
              "Type": "TypeInt",
              "Computed": true
            },
            "host": {
              "Type": "TypeString",
              "Computed": true
            },
            "ipaddress": {
              "Type": "TypeString",
              "Computed": true
            },
            "service_port": {
              "Type": "TypeInt",
              "Computed": true
            }
          }
        }
      },
      "ports": {
        "Type": "TypeList",
        "Required": true,
        "MaxItems": 20,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "number": {
              "Type": "TypeInt",
              "Optional": true
            },
            "protocol": {
              "Type": "TypeString",
              "Optional": true
            }
          }
        }
      }
    }
  },
  "data-sources": {}
}