      "Type": "TypeString",
      "Optional": true,
      "Description": "API Key used to authenticate with the rancher server",
      "Default": {
        "Type": "string"
      }
    },
    "api_url": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The URL to the rancher API, must include version uri (ie. v1 or v2-beta)",
      "Default": {
        "Type": "string"
      }
