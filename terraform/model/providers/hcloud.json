{
  "name": "hcloud",
  "type": "provider",
  "version": "v1.2.0-5-gf978c2c",
  "provider": {
    "endpoint": {
      "Type": "TypeString",
      "Optional": true
    },
    "token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The API token to access the hetzner cloud."
    }
  },
  "resources": {
    "hcloud_floating_ip": {
      "description": {
        "Type": "TypeString",
        "Optional": true
      },
      "home_location": {
        "Type": "TypeString",
        "Optional": true,
        "Computed": true
      },
      "ip_address": {
        "Type": "TypeString",
        "Computed": true
      },
      "server_id": {
        "Type": "TypeInt",
        "Optional": true
      },
      "type": {
        "Type": "TypeString",
        "Required": true
      }
    },
    "hcloud_server": {
      "backup_window": {
        "Type": "TypeString",
        "Optional": true
      },
      "datacenter": {
        "Type": "TypeString",
        "Optional": true,
        "Computed": true
      },
      "image": {
        "Type": "TypeString",
        "Required": true
      },
      "ipv4_address": {
        "Type": "TypeString",
        "Computed": true
      },
      "ipv6_address": {
        "Type": "TypeString",
        "Computed": true
      },
      "iso": {
        "Type": "TypeString",
        "Optional": true
      },
      "keep_disk": {
        "Type": "TypeBool",
        "Optional": true
      },
      "location": {
        "Type": "TypeString",
        "Optional": true,
        "Computed": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "rescue": {
        "Type": "TypeString",
        "Optional": true
      },
      "server_type": {
        "Type": "TypeString",
        "Required": true
      },
      "ssh_keys": {
        "Type": "TypeList",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "TypeString"
        }
      },
      "status": {
        "Type": "TypeString",
        "Computed": true
      },
      "user_data": {
        "Type": "TypeString",
        "Optional": true
      }
    },
    "hcloud_ssh_key": {
      "fingerprint": {
        "Type": "TypeString",
        "Computed": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "public_key": {
        "Type": "TypeString",
        "Required": true
      }
    }
  },
  "data-sources": {}
}