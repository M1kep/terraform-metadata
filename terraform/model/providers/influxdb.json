{
  ".schema_version": "1",
  "name": "influxdb",
  "type": "provider",
  "version": "v1.1.0",
  "provider": {
    "password": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(INFLUXDB_PASSWORD)"
    },
    "skip_ssl_verify": {
      "Type": "Bool",
      "Optional": true,
      "DefaultFunc": "ENV(INFLUXDB_SKIP_SSL_VERIFY)"
    },
    "url": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(INFLUXDB_URL)"
    },
    "username": {
      "Type": "String",
      "Optional": true,
      "DefaultFunc": "ENV(INFLUXDB_USERNAME)"
    }
  },
  "resources": {
    "influxdb_continuous_query": {
      "database": {
        "Type": "String",
        "Required": true
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "query": {
        "Type": "String",
        "Required": true
      }
    },
    "influxdb_database": {
      "name": {
        "Type": "String",
        "Required": true
      },
      "retention_policies": {
        "Type": "List",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "default": {
              "Type": "Bool",
              "Optional": true,
              "Default": {
                "Type": "bool",
                "Value": "false"
              }
            },
            "duration": {
              "Type": "String",
              "Required": true
            },
            "name": {
              "Type": "String",
              "Required": true
            },
            "replication": {
              "Type": "Int",
              "Optional": true,
              "Default": {
                "Type": "int",
                "Value": "1"
              }
            }
          }
        }
      }
    },
    "influxdb_user": {
      "admin": {
        "Type": "Bool",
        "Optional": true,
        "Computed": true
      },
      "grant": {
        "Type": "Set",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "database": {
              "Type": "String",
              "Required": true
            },
            "privilege": {
              "Type": "String",
              "Required": true
            }
          }
        }
      },
      "name": {
        "Type": "String",
        "Required": true
      },
      "password": {
        "Type": "String",
        "Required": true
      }
    }
  },
  "data-sources": {}
}