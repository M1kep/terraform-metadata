{
  "name": "scaleway",
  "type": "provider",
  "version": "v1.5.0-1-g0a7d761",
  "provider": {
    "access_key": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The API key for Scaleway API operations.",
      "Deprecated": "Use `token` instead."
    },
    "organization": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The Organization ID (a.k.a. 'access key') for Scaleway API operations."
    },
    "region": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The Scaleway API region to use.",
      "Default": {
        "Type": "string",
        "Value": "par1"
      }
    },
    "token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The API key for Scaleway API operations."
    }
  },
  "resources": {
    "scaleway_ip": {
      "ip": {
        "Type": "TypeString",
        "Description": "The ipv4 address of the ip",
        "Computed": true
      },
      "reverse": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The ipv4 reverse dns"
      },
      "server": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The server associated with the ip",
        "Computed": true
      }
    },
    "scaleway_security_group": {
      "description": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The description of the security group"
      },
      "enable_default_security": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "Add default security group rules"
      },
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The name of the security group"
      }
    },
    "scaleway_security_group_rule": {
      "action": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The action to take when the security group rule is triggered (accept or drop)"
      },
      "direction": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The direction the traffic is affected (inbound or outbound)"
      },
      "ip_range": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The ip range affected by the security group rule"
      },
      "port": {
        "Type": "TypeInt",
        "Optional": true,
        "Description": "The port affected by the security group rule"
      },
      "protocol": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The protocol of the security group rule (ICMP, TCP or UDP)"
      },
      "security_group": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The security group this rule is attached to"
      }
    },
    "scaleway_server": {
      "boot_type": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The boot_type of the server",
        "Computed": true
      },
      "bootscript": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The boot configuration of the server"
      },
      "dynamic_ip_required": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "determines if a public IP address should be allocated for the server"
      },
      "enable_ipv6": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "determines if IPv6 is enabled for the server"
      },
      "image": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The base image of the server"
      },
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The name of the server"
      },
      "private_ip": {
        "Type": "TypeString",
        "Description": "the Scaleway internal IP address of the server",
        "Computed": true
      },
      "public_ip": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "the public IPv4 address of the server",
        "Computed": true
      },
      "public_ipv6": {
        "Type": "TypeString",
        "Description": "the public IPv6 address of the server, if enabled",
        "Computed": true
      },
      "security_group": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The security group the server is attached to"
      },
      "state": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "the server state (running, stopped)",
        "Computed": true
      },
      "state_detail": {
        "Type": "TypeString",
        "Description": "scaleway description of the server state",
        "Computed": true
      },
      "tags": {
        "Type": "TypeList",
        "Optional": true,
        "Description": "The tags associated with the server",
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "TypeString"
        }
      },
      "type": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The instance type of the server"
      },
      "volume": {
        "Type": "TypeList",
        "Optional": true,
        "Description": "Volumes attached to the server on creation",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "size_in_gb": {
              "Type": "TypeInt",
              "Required": true
            },
            "type": {
              "Type": "TypeString",
              "Required": true
            },
            "volume_id": {
              "Type": "TypeString",
              "Computed": true
            }
          }
        }
      }
    },
    "scaleway_ssh_key": {
      "key": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The ssh key"
      }
    },
    "scaleway_token": {
      "description": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The token description."
      },
      "email": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The account email. Defaults to registered user.",
        "Computed": true
      },
      "expiration_date": {
        "Type": "TypeString",
        "Description": "The tokens expiration date",
        "Computed": true
      },
      "expires": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "Defines if the token is set to expire"
      },
      "password": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "User password, in case a login is require"
      },
      "user_id": {
        "Type": "TypeString",
        "Description": "The userid of the associated user.",
        "Computed": true
      }
    },
    "scaleway_user_data": {
      "key": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The key of the user data to manage"
      },
      "server": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The server the meta data is associated with"
      },
      "value": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The value of the user"
      }
    },
    "scaleway_volume": {
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the name of the volume"
      },
      "server": {
        "Type": "TypeString",
        "Description": "the server the volume is attached to",
        "Computed": true
      },
      "size_in_gb": {
        "Type": "TypeInt",
        "Required": true,
        "Description": "the size of the volume in GB"
      },
      "type": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the type of backing storage"
      }
    },
    "scaleway_volume_attachment": {
      "server": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the server a volume should be attached to"
      },
      "volume": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the volume to attach"
      }
    }
  },
  "data-sources": {
    "scaleway_bootscript": {
      "architecture": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "architecture of the desired bootscript",
        "Computed": true
      },
      "boot_cmd_args": {
        "Type": "TypeString",
        "Description": "commandline boot options used",
        "Computed": true
      },
      "dtb": {
        "Type": "TypeString",
        "Description": "path to Device Tree Blob detailing hardware information",
        "Computed": true
      },
      "initrd": {
        "Type": "TypeString",
        "Description": "URL to initial ramdisk content",
        "Computed": true
      },
      "kernel": {
        "Type": "TypeString",
        "Description": "URL to used kernel",
        "Computed": true
      },
      "name": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "exact name of the desired bootscript"
      },
      "name_filter": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "partial name of the desired bootscript to filter with"
      },
      "organization": {
        "Type": "TypeString",
        "Description": "organization owning the bootscript",
        "Computed": true
      },
      "public": {
        "Type": "TypeBool",
        "Description": "indication if the bootscript is public",
        "Computed": true
      }
    },
    "scaleway_image": {
      "architecture": {
        "Type": "TypeString",
        "Required": true,
        "Description": "architecture of the desired image"
      },
      "creation_date": {
        "Type": "TypeString",
        "Description": "date when the image was created",
        "Computed": true
      },
      "name": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "exact name of the desired image",
        "Computed": true
      },
      "name_filter": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "partial name of the desired image to filter with"
      },
      "organization": {
        "Type": "TypeString",
        "Description": "organization owning the bootscript",
        "Computed": true
      },
      "public": {
        "Type": "TypeBool",
        "Description": "indication if the bootscript is public",
        "Computed": true
      }
    }
  }
}