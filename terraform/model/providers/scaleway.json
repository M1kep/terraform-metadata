{
  "name": "scaleway",
  "type": "provider",
  "version": "v1.0.0-2-gf58fac6",
  "provider": {
    "access_key": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The API key for Scaleway API operations.",
      "Deprecated": "Use `token` instead.",
      "Default": {},
      "Elem": {}
    },
    "organization": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The Organization ID (a.k.a. 'access key') for Scaleway API operations.",
      "Default": {},
      "Elem": {}
    },
    "region": {
      "Type": "TypeString",
      "Optional": true,
      "Description": "The Scaleway API region to use.",
      "Default": {
        "Type": "string",
        "Value": "par1"
      },
      "Elem": {}
    },
    "token": {
      "Type": "TypeString",
      "Required": true,
      "Description": "The API key for Scaleway API operations.",
      "Default": {},
      "Elem": {}
    }
  },
  "resources": {
    "scaleway_ip": {
      "ip": {
        "Type": "TypeString",
        "Description": "The ipv4 address of the ip",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "server": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The server associated with the ip",
        "Computed": true,
        "Default": {},
        "Elem": {}
      }
    },
    "scaleway_security_group": {
      "description": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The description of the security group",
        "Default": {},
        "Elem": {}
      },
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The name of the security group",
        "Default": {},
        "Elem": {}
      }
    },
    "scaleway_security_group_rule": {
      "action": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The action to take when the security group rule is triggered (accept or drop)",
        "Default": {},
        "Elem": {}
      },
      "direction": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The direction the traffic is affected (inbound or outbound)",
        "Default": {},
        "Elem": {}
      },
      "ip_range": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The ip range affected by the security group rule",
        "Default": {},
        "Elem": {}
      },
      "port": {
        "Type": "TypeInt",
        "Optional": true,
        "Description": "The port affected by the security group rule",
        "Default": {},
        "Elem": {}
      },
      "protocol": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The protocol of the security group rule (ICMP, TCP or UDP)",
        "Default": {},
        "Elem": {}
      },
      "security_group": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The security group this rule is attached to",
        "Default": {},
        "Elem": {}
      }
    },
    "scaleway_server": {
      "bootscript": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The boot configuration of the server",
        "Default": {},
        "Elem": {}
      },
      "dynamic_ip_required": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "determines if a public IP address should be allocated for the server",
        "Default": {},
        "Elem": {}
      },
      "enable_ipv6": {
        "Type": "TypeBool",
        "Optional": true,
        "Description": "determines if IPv6 is enabled for the server",
        "Default": {},
        "Elem": {}
      },
      "image": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The base image of the server",
        "Default": {},
        "Elem": {}
      },
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The name of the server",
        "Default": {},
        "Elem": {}
      },
      "private_ip": {
        "Type": "TypeString",
        "Description": "the Scaleway internal IP address of the server",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "public_ip": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "the public IPv4 address of the server",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "public_ipv6": {
        "Type": "TypeString",
        "Description": "the public IPv6 address of the server, if enabled",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "security_group": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "The security group the server is attached to",
        "Default": {},
        "Elem": {}
      },
      "state": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "the server state (running, stopped)",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "state_detail": {
        "Type": "TypeString",
        "Description": "scaleway description of the server state",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "tags": {
        "Type": "TypeList",
        "Optional": true,
        "Description": "The tags associated with the server",
        "Default": {},
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "TypeString"
        }
      },
      "type": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The instance type of the server",
        "Default": {},
        "Elem": {}
      },
      "volume": {
        "Type": "TypeList",
        "Optional": true,
        "Description": "Volumes attached to the server on creation",
        "Default": {},
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "size_in_gb": {
              "Type": "TypeInt",
              "Required": true,
              "Default": {},
              "Elem": {}
            },
            "type": {
              "Type": "TypeString",
              "Required": true,
              "Default": {},
              "Elem": {}
            },
            "volume_id": {
              "Type": "TypeString",
              "Computed": true,
              "Default": {},
              "Elem": {}
            }
          }
        }
      }
    },
    "scaleway_volume": {
      "name": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the name of the volume",
        "Default": {},
        "Elem": {}
      },
      "server": {
        "Type": "TypeString",
        "Description": "the server the volume is attached to",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "size_in_gb": {
        "Type": "TypeInt",
        "Required": true,
        "Description": "the size of the volume in GB",
        "Default": {},
        "Elem": {}
      },
      "type": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the type of backing storage",
        "Default": {},
        "Elem": {}
      }
    },
    "scaleway_volume_attachment": {
      "server": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the server a volume should be attached to",
        "Default": {},
        "Elem": {}
      },
      "volume": {
        "Type": "TypeString",
        "Required": true,
        "Description": "the volume to attach",
        "Default": {},
        "Elem": {}
      }
    }
  },
  "data-sources": {
    "scaleway_bootscript": {
      "architecture": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "architecture of the desired bootscript",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "boot_cmd_args": {
        "Type": "TypeString",
        "Description": "commandline boot options used",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "dtb": {
        "Type": "TypeString",
        "Description": "path to Device Tree Blob detailing hardware information",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "initrd": {
        "Type": "TypeString",
        "Description": "URL to initial ramdisk content",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "kernel": {
        "Type": "TypeString",
        "Description": "URL to used kernel",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "name": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "exact name of the desired bootscript",
        "Default": {},
        "Elem": {}
      },
      "name_filter": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "partial name of the desired bootscript to filter with",
        "Default": {},
        "Elem": {}
      },
      "organization": {
        "Type": "TypeString",
        "Description": "organization owning the bootscript",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "public": {
        "Type": "TypeBool",
        "Description": "indication if the bootscript is public",
        "Computed": true,
        "Default": {},
        "Elem": {}
      }
    },
    "scaleway_image": {
      "architecture": {
        "Type": "TypeString",
        "Required": true,
        "Description": "architecture of the desired image",
        "Default": {},
        "Elem": {}
      },
      "creation_date": {
        "Type": "TypeString",
        "Description": "date when the image was created",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "name": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "exact name of the desired image",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "name_filter": {
        "Type": "TypeString",
        "Optional": true,
        "Description": "partial name of the desired image to filter with",
        "Default": {},
        "Elem": {}
      },
      "organization": {
        "Type": "TypeString",
        "Description": "organization owning the bootscript",
        "Computed": true,
        "Default": {},
        "Elem": {}
      },
      "public": {
        "Type": "TypeBool",
        "Description": "indication if the bootscript is public",
        "Computed": true,
        "Default": {},
        "Elem": {}
      }
    }
  }
}