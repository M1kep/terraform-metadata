{
  "name": "triton",
  "type": "provider",
  "schema": {
    "account": [
      {
        "name": "Type",
        "type": "schema.ValueType",
        "value": "TypeString"
      },
      {
        "name": "Required",
        "type": "bool",
        "value": "true"
      }
    ],
    "key_id": [
      {
        "name": "Type",
        "type": "schema.ValueType",
        "value": "TypeString"
      },
      {
        "name": "Required",
        "type": "bool",
        "value": "true"
      }
    ],
    "key_material": [
      {
        "name": "Type",
        "type": "schema.ValueType",
        "value": "TypeString"
      },
      {
        "name": "Required",
        "type": "bool",
        "value": "true"
      }
    ],
    "url": [
      {
        "name": "Type",
        "type": "schema.ValueType",
        "value": "TypeString"
      },
      {
        "name": "Required",
        "type": "bool",
        "value": "true"
      }
    ]
  },
  "resources": {
    "triton_firewall_rule": {
      "enabled": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeBool"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Default",
          "type": "interface {}",
          "value": "false"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "Indicates if the rule is enabled"
        }
      ],
      "rule": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Required",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "firewall rule text"
        }
      ]
    },
    "triton_key": {
      "key": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Required",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "content of public key from disk"
        }
      ],
      "name": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "name of this key (will be generated from the key comment, if not set and comment present)"
        }
      ]
    },
    "triton_machine": {
      "administrator_pw": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "administrator's initial password (Windows only)"
        }
      ],
      "created": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "when the machine was created"
        }
      ],
      "dataset": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "dataset URN the machine was provisioned with"
        }
      ],
      "disk": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeInt"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "amount of disk the machine has (in Gb)"
        }
      ],
      "firewall_enabled": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeBool"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Default",
          "type": "interface {}",
          "value": "false"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "enable firewall for this machine"
        }
      ],
      "image": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Required",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "image UUID"
        }
      ],
      "ips": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeList"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "IP addresses the machine has"
        },
        {
          "name": "Elem",
          "type": "ResourceSchemaElements",
          "value": [
            {
              "name": "Type",
              "type": "schema.ValueType",
              "value": "TypeString"
            }
          ]
        }
      ],
      "memory": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeInt"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "amount of memory the machine has (in Mb)"
        }
      ],
      "name": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "friendly name"
        }
      ],
      "networks": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeList"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "desired network IDs"
        },
        {
          "name": "Elem",
          "type": "ResourceSchemaElements",
          "value": [
            {
              "name": "Type",
              "type": "schema.ValueType",
              "value": "TypeString"
            }
          ]
        }
      ],
      "package": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Required",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "name of the package to use on provisioning"
        }
      ],
      "primaryip": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "the primary (public) IP address for the machine"
        }
      ],
      "root_authorized_keys": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "authorized keys for the root user on this machine"
        }
      ],
      "state": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "current state of the machine"
        }
      ],
      "tags": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeMap"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "machine tags"
        }
      ],
      "type": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "machine type (smartmachine or virtualmachine)"
        }
      ],
      "updated": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "when the machine was update"
        }
      ],
      "user_data": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "copied to machine on boot"
        }
      ],
      "user_script": [
        {
          "name": "Type",
          "type": "schema.ValueType",
          "value": "TypeString"
        },
        {
          "name": "Optional",
          "type": "bool",
          "value": "true"
        },
        {
          "name": "Description",
          "type": "string",
          "value": "user script to run on boot (every boot on SmartMachines)"
        }
      ]
    }
  }
}
