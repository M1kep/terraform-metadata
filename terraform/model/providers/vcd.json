      },
      "org": {
        "Type": "String",
        "Optional": true
      },
      "ova_path": {
        "Type": "String",
        "Required": true,
        "Description": "absolute or relative path to OVA"
      },
      "show_upload_progress": {
        "Type": "Bool",
        "Optional": true,
        "Description": "shows upload progress in stdout"
      },
      "upload_piece_size": {
        "Type": "Int",
        "Optional": true,
        "Description": "size of upload file piece size in mega bytes",
        "Default": {
          "Type": "int",
          "Value": "1"
