      "name": {
        "Type": "String",
        "Required": true,
        "Description": "media name"
      },
      "org": {
        "Type": "String",
        "Optional": true
      },
      "show_upload_progress": {
        "Type": "Bool",
        "Optional": true,
        "Description": "shows upload progress in stdout"
