{
  "name": "datadog",
  "type": "provider",
  "version": "v1.0.3-9-gb09ab8c",
  "provider": {
    "api_key": {
      "Type": "TypeString",
      "Required": true
    },
    "app_key": {
      "Type": "TypeString",
      "Required": true
    }
  },
  "resources": {
    "datadog_downtime": {
      "active": {
        "Type": "TypeBool",
        "Optional": true
      },
      "disabled": {
        "Type": "TypeBool",
        "Optional": true
      },
      "end": {
        "Type": "TypeInt",
        "Optional": true
      },
      "message": {
        "Type": "TypeString",
        "Optional": true
      },
      "monitor_id": {
        "Type": "TypeInt",
        "Optional": true
      },
      "recurrence": {
        "Type": "TypeList",
        "Optional": true,
        "MaxItems": 1,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "period": {
              "Type": "TypeInt",
              "Required": true
            },
            "type": {
              "Type": "TypeString",
              "Required": true
            },
            "until_date": {
              "Type": "TypeInt",
              "Optional": true,
              "ConflictsWith": [
                "recurrence.until_occurrences"
              ]
            },
            "until_occurrences": {
              "Type": "TypeInt",
              "Optional": true,
              "ConflictsWith": [
                "recurrence.until_date"
              ]
            },
            "week_days": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "TypeString"
              }
            }
          }
        }
      },
      "scope": {
        "Type": "TypeList",
        "Required": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "TypeString"
        }
      },
      "start": {
        "Type": "TypeInt",
        "Optional": true
      }
    },
    "datadog_metric_metadata": {
      "description": {
        "Type": "TypeString",
        "Optional": true
      },
      "metric": {
        "Type": "TypeString",
        "Required": true
      },
      "per_unit": {
        "Type": "TypeString",
        "Optional": true
      },
      "short_name": {
        "Type": "TypeString",
        "Optional": true
      },
      "statsd_interval": {
        "Type": "TypeInt",
        "Optional": true
      },
      "type": {
        "Type": "TypeString",
        "Optional": true
      },
      "unit": {
        "Type": "TypeString",
        "Optional": true
      }
    },
    "datadog_monitor": {
      "escalation_message": {
        "Type": "TypeString",
        "Optional": true
      },
      "evaluation_delay": {
        "Type": "TypeInt",
        "Optional": true,
        "Computed": true
      },
      "include_tags": {
        "Type": "TypeBool",
        "Optional": true
      },
      "locked": {
        "Type": "TypeBool",
        "Optional": true
      },
      "message": {
        "Type": "TypeString",
        "Required": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "new_host_delay": {
        "Type": "TypeInt",
        "Optional": true,
        "Computed": true
      },
      "no_data_timeframe": {
        "Type": "TypeInt",
        "Optional": true
      },
      "notify_audit": {
        "Type": "TypeBool",
        "Optional": true
      },
      "notify_no_data": {
        "Type": "TypeBool",
        "Optional": true
      },
      "query": {
        "Type": "TypeString",
        "Required": true
      },
      "renotify_interval": {
        "Type": "TypeInt",
        "Optional": true
      },
      "require_full_window": {
        "Type": "TypeBool",
        "Optional": true
      },
      "silenced": {
        "Type": "TypeMap",
        "Optional": true,
        "Elem": {
          "Type": "schema.ValueType",
          "Value": "TypeInt"
        }
      },
      "tags": {
        "Type": "TypeList",
        "Optional": true,
        "Elem": {
          "Type": "SchemaElements",
          "ElementsType": "TypeString"
        }
      },
      "thresholds": {
        "Type": "TypeMap",
        "Optional": true,
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "critical": {
              "Type": "TypeFloat",
              "Optional": true
            },
            "critical_recovery": {
              "Type": "TypeFloat",
              "Optional": true
            },
            "ok": {
              "Type": "TypeFloat",
              "Optional": true
            },
            "warning": {
              "Type": "TypeFloat",
              "Optional": true
            },
            "warning_recovery": {
              "Type": "TypeFloat",
              "Optional": true
            }
          }
        }
      },
      "timeout_h": {
        "Type": "TypeInt",
        "Optional": true
      },
      "type": {
        "Type": "TypeString",
        "Required": true
      }
    },
    "datadog_timeboard": {
      "description": {
        "Type": "TypeString",
        "Required": true,
        "Description": "A description of the dashboard's content."
      },
      "graph": {
        "Type": "TypeList",
        "Required": true,
        "Description": "A list of graph definitions.",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "autoscale": {
              "Type": "TypeBool",
              "Optional": true,
              "Description": "Automatically scale graphs"
            },
            "custom_unit": {
              "Type": "TypeString",
              "Optional": true,
              "Description": "Use a custom unit (like 'users')"
            },
            "events": {
              "Type": "TypeList",
              "Optional": true,
              "Description": "Filter for events to be overlayed on the graph.",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "TypeString"
              }
            },
            "group": {
              "Type": "TypeList",
              "Optional": true,
              "Description": "A list of groupings for hostmap type graphs.",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "TypeString"
              }
            },
            "include_no_metric_hosts": {
              "Type": "TypeBool",
              "Optional": true,
              "Description": "Include hosts without metrics in hostmap graphs"
            },
            "include_ungrouped_hosts": {
              "Type": "TypeBool",
              "Optional": true,
              "Description": "Include ungrouped hosts in hostmap graphs"
            },
            "marker": {
              "Type": "TypeList",
              "Optional": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "label": {
                    "Type": "TypeString",
                    "Optional": true
                  },
                  "type": {
                    "Type": "TypeString",
                    "Required": true
                  },
                  "value": {
                    "Type": "TypeString",
                    "Required": true
                  }
                }
              }
            },
            "precision": {
              "Type": "TypeString",
              "Optional": true,
              "Description": "How many digits to show"
            },
            "request": {
              "Type": "TypeList",
              "Required": true,
              "Elem": {
                "Type": "SchemaInfo",
                "Info": {
                  "aggregator": {
                    "Type": "TypeString",
                    "Optional": true
                  },
                  "change_type": {
                    "Type": "TypeString",
                    "Optional": true,
                    "Description": "Type of change for change graphs."
                  },
                  "compare_to": {
                    "Type": "TypeString",
                    "Optional": true,
                    "Description": "The time period to compare change against in change graphs."
                  },
                  "conditional_format": {
                    "Type": "TypeList",
                    "Optional": true,
                    "Description": "A list of conditional formatting rules.",
                    "Elem": {
                      "Type": "SchemaInfo",
                      "Info": {
                        "comparator": {
                          "Type": "TypeString",
                          "Required": true,
                          "Description": "Comparator (\u003c, \u003e, etc)"
                        },
                        "custom_bg_color": {
                          "Type": "TypeString",
                          "Optional": true,
                          "Description": "Custom background color (e.g., #205081)"
                        },
                        "custom_fg_color": {
                          "Type": "TypeString",
                          "Optional": true,
                          "Description": "Custom foreground color (e.g., #59afe1)"
                        },
                        "palette": {
                          "Type": "TypeString",
                          "Optional": true,
                          "Description": "The palette to use if this condition is met."
                        },
                        "value": {
                          "Type": "TypeString",
                          "Optional": true,
                          "Description": "Value that is threshold for conditional format"
                        }
                      }
                    }
                  },
                  "extra_col": {
                    "Type": "TypeString",
                    "Optional": true,
                    "Description": "If set to 'present', this will include the present values in change graphs."
                  },
                  "increase_good": {
                    "Type": "TypeBool",
                    "Optional": true,
                    "Description": "Decides whether to represent increases as good or bad in change graphs."
                  },
                  "order_by": {
                    "Type": "TypeString",
                    "Optional": true,
                    "Description": "The field a change graph will be ordered by."
                  },
                  "order_direction": {
                    "Type": "TypeString",
                    "Optional": true,
                    "Description": "Sort change graph in ascending or descending order."
                  },
                  "q": {
                    "Type": "TypeString",
                    "Required": true
                  },
                  "stacked": {
                    "Type": "TypeBool",
                    "Optional": true
                  },
                  "style": {
                    "Type": "TypeMap",
                    "Optional": true
                  },
                  "type": {
                    "Type": "TypeString",
                    "Optional": true
                  }
                }
              }
            },
            "scope": {
              "Type": "TypeList",
              "Optional": true,
              "Description": "A list of scope filters for hostmap type graphs.",
              "Elem": {
                "Type": "SchemaElements",
                "ElementsType": "TypeString"
              }
            },
            "style": {
              "Type": "TypeMap",
              "Optional": true
            },
            "text_align": {
              "Type": "TypeString",
              "Optional": true,
              "Description": "How to align text"
            },
            "title": {
              "Type": "TypeString",
              "Required": true,
              "Description": "The name of the graph."
            },
            "viz": {
              "Type": "TypeString",
              "Required": true
            },
            "yaxis": {
              "Type": "TypeMap",
              "Optional": true
            }
          }
        }
      },
      "read_only": {
        "Type": "TypeBool",
        "Optional": true
      },
      "template_variable": {
        "Type": "TypeList",
        "Optional": true,
        "Description": "A list of template variables for using Dashboard templating.",
        "Elem": {
          "Type": "SchemaInfo",
          "Info": {
            "default": {
              "Type": "TypeString",
              "Optional": true,
              "Description": "The default value for the template variable on dashboard load."
            },
            "name": {
              "Type": "TypeString",
              "Required": true,
              "Description": "The name of the variable."
            },
            "prefix": {
              "Type": "TypeString",
              "Optional": true,
              "Description": "The tag prefix associated with the variable. Only tags with this prefix will appear in the variable dropdown."
            }
          }
        }
      },
      "title": {
        "Type": "TypeString",
        "Required": true,
        "Description": "The name of the dashboard."
      }
    },
    "datadog_user": {
      "disabled": {
        "Type": "TypeBool",
        "Optional": true
      },
      "email": {
        "Type": "TypeString",
        "Required": true
      },
      "handle": {
        "Type": "TypeString",
        "Required": true
      },
      "is_admin": {
        "Type": "TypeBool",
        "Optional": true
      },
      "name": {
        "Type": "TypeString",
        "Required": true
      },
      "role": {
        "Type": "TypeString",
        "Optional": true
      },
      "verified": {
        "Type": "TypeBool",
        "Computed": true
      }
    }
  },
  "data-sources": {}
}